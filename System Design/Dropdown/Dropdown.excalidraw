      "text": "Menu\n\nconst StyledUL = styled.ul`\n`;\n\nexport default function Menu({ children, className }) {\n  const { onMenuItemSelect, onClose, open } = useContext(Context);\n  const ref = useRef();\n\n  const onKeyPress = (event) => {\n    // Move focus to elements\n   // Close when esc is pressed\n  };\n\n  const onClick = (e) => {\n    // get clicked item value and pass it to consumer\n    // onMenuItemSelect(<value>)\n  };\n\n  return (\n    <div>\n      <StyledUL\n        className={`menu ${className}`}\n        ref={ref}\n        onKeyDown={onKeyPress}\n        onClick={onClick}\n      >\n        {children}\n      </StyledUL>\n    </div>\n  );\n}\n",
